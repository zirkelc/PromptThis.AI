{
	"compilerOptions": {
		"lib": [
			"es2022",
			"dom",
			"dom.iterable",
		],
		"target": "es6",
		"module": "ESNext",
		"moduleResolution": "Node",
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"sourceMap": true,
		"allowJs": true,
		"noEmit": true
	},
	"include": [
		"**/*.ts",
	]
}